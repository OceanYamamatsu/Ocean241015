def is_subsequence(s, t):
    # sの文字列インデックス
    i = 0
    # tの文字列インデックス
    j = 0

    # sとtの文字列を比較
    while i < len(s) and j < len(t):
        # 文字が一致した場合、次の文字を比較するためにiとjをインクリメント
        if s[i] == t[j]:
            i += 1
        j += 1

    # sのすべての文字が見つかった場合、tはsの部分文字列
    return i == len(s)

# テスト用の例
S = input().strip()
T = input().strip()
if is_subsequence(T, S):
    print("Yes")
else:
    print("No")






