syo = 998244353

def main():
    N = int(input())
    ans = 0
    base = 1
    for digit_count in range(1, len(str(N)) + 1):
        if base * 10 <= N:
            ans += base * 9 * digit_count
            ans %= syo
            base *= 10
        else:
            ans += (N - base + 1) * digit_count
            ans %= syo
            break
    print(ans)

if __name__ == "__main__":
    main()

